{
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2018
  },
  "extends": [
    "eslint:recommended",
    "plugin:prettier/recommended"
  ],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "quotes": [2, "single"]
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "root": true,
  "env": {
    "browser": true,
    "node": true,
    "jest": true,
    "jquery": true,
    "es6": true
  }
}